fileManifest=""

addThis () {
    fileManifest+=" $1"
}

addThis dirtree
addThis getBrew
addThis getMyGits
addThis getRPMDefs

type brew &> /dev/null && addThis update

[[ -d /etc/selinux ]] && addThis getSELinuxDefs

[[ -d ~/ldap ]] && addThis ldap
[[ -d ~/docker ]] && addThis myDocker
[[ -d ~/camunda ]] && addThis cammng
[[ -d ~/camunda ]] && addThis wtfCleanUp
[[ -d ~/camunda ]] && addThis wtfFind

